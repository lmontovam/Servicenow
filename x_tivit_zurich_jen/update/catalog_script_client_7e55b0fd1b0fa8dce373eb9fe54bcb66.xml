        <script><![CDATA[function onSubmit() {
	if(!(g_form.getValue('sql_command')) && this.document.getElementsByClassName('get-attachment').length == 0){
		g_form.addErrorMessage(getMessage('Anexo obrigatório!'));
		return false;
	}
	if((g_form.getValue('sql_command')) && this.document.getElementsByClassName('get-attachment').length != 0){
		g_form.clearValue('sql_command');
	}
	if(g_form.getValue('start_date') >= g_form.getValue('end_date')){
		g_form.addErrorMessage(getMessage('Data de Terminio Planejada deve maior que a Data de Início Planejada'));
		return false;
	}
}]]></script>
